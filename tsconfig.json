{
  "compileOnSave": false,
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "baseUrl": "./src",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "es2015",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "noLib": false,
    "pretty": false,
    "removeComments": true,
    "sourceMap": false,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types",
      "node_modules",
      "tools/manual_typings"
    ],
    "types": [
      "jquery",
      "googlemaps"
    ]
  },
  "exclude": [
    "node_modules",
    "*-prod.ts"
  ],
  "include": [
    "src/**/*",
    "tools/manual_typings/index.d.ts"
  ]
}
